<p id="notice"><%= notice %></p>

<h1>Our Agencies</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>City</th>
      <th>State</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @agencies.each do |agency| %>
      <tr>
        <td><%= agency.name %></td>
        <td><%= agency.email %></td>
        <td><%= agency.city %></td>
        <td><%= agency.state %></td>
        <td><%= link_to 'Show', agency %></td>
      </tr>
    <% end %>
  </tbody>
</table>